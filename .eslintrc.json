{
    "extends": "airbnb",
    "globals": {
      "describe": true,
      "expect": true,
      "it": true,
      "test": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    },
    "plugins": [
        "standard",
        "promise"
    ],
    "rules": {
      "arrow-parens": [2, "as-needed"],
      "max-len": [2, 100]
    }
}
