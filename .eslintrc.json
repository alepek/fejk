{
    "extends": "airbnb",
    "env": {
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "plugins": [
        "standard",
        "promise"
    ],
    "rules": {
      "consistent-return": 0,
      "arrow-parens": [2, "as-needed"],
      "max-len": [2, 100]
    }
}
